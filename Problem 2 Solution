import java.io.*;
import java.util.*;
class A{
    public void fun(){
        System.out.println("Hello I am a motorcycle, I am a cycle with an engine.");
    }
}
class B extends A{
    public void supfunc(){
        super.fun();
        System.out.println("My ancestor is a cycle who is a vehicle with pedals.");
    }
}
public class Solution {

    public static void main(String[] args) {
        B b=new B();
        b.supfunc();
    }
}
